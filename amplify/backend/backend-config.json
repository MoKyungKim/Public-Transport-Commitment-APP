{

	"storage": {
		"user": {
			"service": "DynamoDB",
			"providerPlugin": "awscloudformation"
		}
	},
	"function": {
		"userlambda": {
			"build": true,
			"providerPlugin": "awscloudformation",
			"service": "Lambda",
			"dependsOn": [
				{
					"category": "storage",
					"resourceName": "user",
					"attributes": [
						"Name",
						"Arn"
					]
				}
			]
		}
	}
}